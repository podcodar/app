    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "prisma-erd-generator": "^1.7.0",
