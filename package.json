{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fmt": "prettier --write . && next lint --fix",
    "lint-staged": "lint-staged",
    "lint": "next lint",
    "test": "jest --watchAll",
    "analyze": "ANALYZE=true pnpm build",
    "db:generate": "prisma generate",
